   "source": [
    "SELECT\n",
    "    c.customer_name,\n",
    "    COUNT(o.order_id) AS order_count\n",
    "FROM\n",
    "    Customers c\n",
    "JOIN\n",
    "    Orders o ON c.customer_id = o.customer_id\n",
    "GROUP BY\n",
    "    c.customer_name\n",
    "ORDER BY\n",
    "    order_count DESC,\n",
    "    c.customer_name ASC\n",
    "LIMIT 5;\n"
   ]
  }
 ],
